{
  "name": "feishu-script-web",
  "private": true,
  "type": "module",
  "output": "dist",
  "scripts": {
    "build": "nuxt build && node scripts/post-build.cjs",
    "build:prod": "nuxt build --config nuxt.config.prod.ts && node scripts/post-build.cjs",
    "build:safe": "nuxt build && node scripts/post-build.cjs",
    "build:only": "nuxt build",
    "dev": "nuxt dev",
    "dev:ip": "nuxt dev --host 0.0.0.0",
    "start": "node dist/server/index.mjs",
    "start:ip": "node start-with-ip.js",
    "pm2:start": "pm2 start ecosystem.config.cjs",
    "pm2:stop": "pm2 stop feishu-script",
    "pm2:restart": "pm2 restart feishu-script",
    "pm2:logs": "pm2 logs feishu-script",
    "pm2:status": "pm2 status",
    "pm2:https": "pm2 start ecosystem.config.https.cjs",
    "pm2:dev-prod": "pm2 start ecosystem.config.dev-prod.cjs",
    "package": "node create-deploy-package.js",
    "setup-ssl": "bash setup-https.sh",
    "fix-auth": "bash fix-build-auth.sh",
    "build-package": "node build-and-package.js",
    "post-build": "node scripts/post-build.cjs",
    "cleanup": "node cleanup-removed-files.js",
    "generate": "nuxt generate",
    "generate:static": "nuxt generate --config nuxt.config.static.ts",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@element-plus/nuxt": "^1.0.9",
    "@lark-base-open/js-sdk": "^0.4.0",
    "@pinia/nuxt": "^0.5.1",
    "element-plus": "^2.5.6",
    "nuxt": "^3.14.159",
    "pinia": "^2.1.7",
    "vue": "^3.4.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "typescript": "^5.3.0",
    "vue-tsc": "^3.2.3"
  }
}
